{
  "name": "js-reporters",
  "version": "1.2.3",
  "description": "Common reporter interface for JavaScript testing frameworks.",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-reporters/js-reporters.git"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "semistandard",
    "test": "npm run lint && npm run test-unit && npm run test-integration",
    "test-unit": "qunit 'test/unit/*.js'",
    "test-integration": "qunit test/integration/adapters.js",
    "test-versions": "qunit test/versions/versions.js"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "commitplease": "3.1.0",
    "jasmine": "2.5.1",
    "mocha": "^2.4.5",
    "qunit": "^2.11.1",
    "qunitjs": "1.23.1",
    "semistandard": "14.2.3",
    "semver": "^7.3.2",
    "sinon": "^1.17.4"
  },
  "commitplease": {
    "nohook": true,
    "components": [
      "Build",
      "Changelog",
      "Readme",
      "Release",
      "Reporter",
      "Test",
      "Spec",
      "Docs"
    ]
  }
}
